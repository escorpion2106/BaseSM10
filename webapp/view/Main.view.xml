<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="Zsm10.controller.Main" displayBlock="true">
	<App id="app">
		<pages>
			<Page title="{i18n>title}" showNavButton="true" navButtonPress="onNavBack">
				<content>
					<Table id="table_imputacion" growing="true" mode="SingleSelectMaster" selectionChange="click"
						items="{ path: '/HorasSet'}">
						<headerToolbar></headerToolbar>
						<columns>
							<Column visible="false">
								<Text text="RecordId"/>
							</Column>
							<Column>
								<Text text="ZzobjectId"/>
							</Column>
							<Column>
								<Text text="ZzproySra"/>
							</Column>
							<Column>
								<Text text="Zzusuario"/>
							</Column>
							<Column>
								<Text text="ZzfImputacion"/>
							</Column>
						</columns>
						<items>
							<ColumnListItem>
								<cells>
									<Text text="{RecordId}"/>
									<Text text="{ZzobjectId}"/>
									<Text text="{ZzproySra}"/>
									<Text text="{Zzusuario}"/>
									<Text text="{ path : 'ZzfImputacion', type: 'sap.ui.model.type.DateTime', formatOptions: { style : 'medium'}}"/>
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>